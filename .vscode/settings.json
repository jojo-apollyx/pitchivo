{
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": false,
  "deno.path": "/Users/jinglz/.deno/bin/deno",
  "deno.suggest.imports.hosts": {
    "https://deno.land": true,
    "https://esm.sh": true
  },
  "deno.suggest.imports.autoDiscover": true,
  "deno.importMap": "./deno.json",
  "deno.enablePaths": [
    "supabase/functions"
  ],
